{
  "name": "v-resize-observer",
  "version": "1.4.0",
  "description": "Resize observer for Vue.",
  "main": "dist/ResizeObserver.umd.js",
  "module": "dist/ResizeObserver.esm.js",
  "unpkg": "dist/ResizeObserver.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meqn/v-resize-observer.git"
  },
  "keywords": [
    "vue-resizse",
    "vue-component",
    "vue-directive",
    "vue",
    "resize",
    "ResizeObserver",
    "vue-resize-observer"
  ],
  "author": "Mervin <mengqing723@gmail.com>",
  "bugs": {
    "url": "https://github.com/Meqn/v-resize-observer/issues"
  },
  "scripts": {
    "dev": "vue-cli-service serve ./demo/main.js",
    "build": "NODE_ENV=production npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "license": "MIT",
